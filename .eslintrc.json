{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "no-underscore-dangle": "off",
    "no-console": "off",
    "camelcase": "off"
  }
}
