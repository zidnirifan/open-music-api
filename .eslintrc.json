{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-console": "off",
    "newline-per-chained-call": "off",
    "object-curly-newline": "off"
  }
}
